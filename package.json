{
  "name": "vue-route-generator",
  "version": "0.1.0",
  "author": "katashin",
  "description": "Vue Router route config generator",
  "keywords": [
    "Vue",
    "Vue Router",
    "routing",
    "generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/ktsn/vue-route-generator",
  "bugs": "https://github.com/ktsn/vue-route-generator/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vue-route-generator.git"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "clean": "rm -rf lib",
    "build": "tsc -p src",
    "dev": "jest --watch",
    "lint": "tslint -p . && prettier --list-different \"{src,scripts,test}/**/*.{js,ts}\"",
    "format": "prettier --write \"{src,scripts,test}/**/*.{js,ts}\"",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "test/tsconfig.json"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^10.5.2",
    "@types/prettier": "^1.13.2",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-ktsn": "^2.1.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "dependencies": {
    "fast-glob": "^2.2.2",
    "prettier": "1.11.1"
  },
  "peerDependencies": {
    "vue-template-compiler": "^2.0.0"
  }
}
